        });
        $('#art_id').change(function(){
            var cat = $("#art_id").val();
            $.ajax({
                url: '/stock/inventaire/vente',
                type: 'POST',
                cache: false,
                data: {
                    id: cat
                },
                success: function (data) {
                    //alert(data);
                    $("#prix_vente").html(data.vente);
                     $("#prix_achat").html(data.achat);
                }
            });
            
        });
        $('#filter').change(function(){
