    <script>
        $(document).ready(function(){
       $("#myInput").on("keyup", function() {
                var value = $(this).val().toLowerCase();
                var art = $(".myArt");
                $('.myArt').each(function() { 
                    if( $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)) {  
                        $(this.art).show(); 
                    } else {
                        $(this.art).hide(); 
                    }
                });
            });
            var qrcode = new QRCode(document.querySelector('#qrcode'), {
                width : 100,
                height : 100
            });
            function makeCode () {		
                var elText = document.querySelector('#qrId');
                
                if (!elText.value) {
                    elText.focus();
                    return;
                }
                
                qrcode.makeCode(elText.value);
            }
            makeCode();
            $("#qrId").
                on("blur", function () {
                    makeCode();
                }).
                on("keydown", function (e) {
                    if (e.keyCode == 13) {
                        makeCode();
                    }
                });
            
           
        });
    </script>
